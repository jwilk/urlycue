[MASTER]
load-plugins = pylint.extensions.check_elif

[MESSAGES CONTROL]
disable =
    bad-builtin,
    bad-continuation,
    fixme,
    invalid-name,
    locally-disabled,
    logging-format-interpolation,
    no-else-return,
    redefined-variable-type,
    too-few-public-methods,

[REPORTS]
reports = no
score = no
msg-template = {C}: {path}:{line}: {symbol} [{obj}] {msg}

[FORMAT]
max-line-length = 120
expected-line-ending-format = LF

